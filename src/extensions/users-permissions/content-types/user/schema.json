{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "hasAcceptedTerms": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "pacLink": {
      "type": "string"
    },
    "firstPacStatus": {
      "type": "enumeration",
      "enum": [
        "READY",
        "DONE",
        "WAITING",
        "INVALID",
        "UNAVAILABLE"
      ],
      "default": "READY"
    },
    "finalPacStatus": {
      "type": "enumeration",
      "enum": [
        "READY",
        "DONE",
        "WAITING",
        "INVALID",
        "UNAVAILABLE"
      ],
      "default": "UNAVAILABLE"
    },
    "additionalData": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::additional-data.additional-data"
    },
    "name": {
      "type": "string"
    },
    "userProgress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-progress.user-progress",
      "inversedBy": "user"
    },
    "isAdmin": {
      "type": "boolean",
      "default": false
    }
  }
}
